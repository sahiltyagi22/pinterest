<%- include('./partials/header.ejs') %>

<div class="w-full min-h-screen bg-zinc-800 px-10 py-8 text-white">
    <div class="cards flex flex-wrap gap-10">
        <% posts.forEach(element => { %>
             <div class=" card w-32 ">
            <div class=" w-full h-40 bg-zinc-700 rounded-lg">
                <img class="w-full h-full object-cover" src="/images/uploads/<%= element.image %>" alt="" srcset="">
            </div>
            <h5 class="mt-2 text-sm"><%= element.title %></h5>
            <h6 class="text-xs opacity-60 ">@<%= element.user.username %></h6>
        </div>
         <% }); %>
       
    </div>

</div>